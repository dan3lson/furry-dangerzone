<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1,
                                shrink-to-fit=no">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <%= display_meta_tags site: "Leksi" %>
  <meta name="author" content="Danelson Rosa">
  <meta name="google-site-verification" content="UA-55816562-1">
  <meta property="og:site_name" content="Leksi" />
  <meta property="og:url" content="https://www.leksi.education/" />
  <meta property="og:type" content="website" />
  <meta property="og:image" content="https://www.leksi.education/assets/pup2-8928c81e06050f48bf7a0e71a3245545b64407b55eea3001c2e37c49ed906907.png" />
  <meta property="fb:app_id" content="549152605232993" />
  <meta name="twitter:site" content="Leksi" />
  <meta name="twitter:url" content="https://www.leksi.education/" />
  <meta name="twitter:image" content="https://www.leksi.education/assets/pup2-8928c81e06050f48bf7a0e71a3245545b64407b55eea3001c2e37c49ed906907.png" />
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">

  <%= render "layouts/iOS_icons" %>
  <%= render "layouts/iOS_splash_screens" %>

  <%= favicon_link_tag 'favicon.ico' %>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= stylesheet_link_tag    'https://cdnjs.cloudflare.com/ajax/libs/flickity/1.1.0/flickity.min.css' %>
  <%= stylesheet_link_tag    'https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.1/animate.min.css' %>
  <%= stylesheet_link_tag    'https://afeld.github.io/emoji-css/emoji.css' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>

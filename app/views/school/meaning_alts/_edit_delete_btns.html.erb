<div class="float-right">
	<%= button_tag name: nil,
								 type: "button",
								 class: "btn btn-warning btn-sm mean-alt-edit-btn",
								 remote: true do
	%>
		<i class="fa fa-edit"></i>
		Edit
	<% end %>
	<%= button_to school_meaning_alt_path(m_a),
								class: "btn btn-danger btn-sm mean-alt-del-form",
								method: :delete,
								remote: true do
	%>
		<i class="fa fa-trash"></i> Delete
	<% end %>
</div>

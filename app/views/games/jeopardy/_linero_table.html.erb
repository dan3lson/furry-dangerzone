<table class="table table-bordered table-sm" id="linero-table">
	<thead>
		<tr class="bg-primary text-white">
			<th class="text-center">Meanings</th>
			<th class="text-center">Synonyms</th>
			<th class="text-center">Antonyms</th>
			<th class="text-center">Examples</th>
			<th class="text-center">Syllables</th>
		</tr>
	</thead>
	<tbody class="text-center">
		<%= render "games/jeopardy/linero_row",
			amount: 300,
			row: rows[0]
		%>
		<%= render "games/jeopardy/linero_row",
			amount: 600,
			row: rows[1]
		%>
		<%= render "games/jeopardy/linero_row",
			amount: 900,
			row: rows[2]
		%>
		<%= render "games/jeopardy/linero_row",
			amount: 1200,
			row: rows[3]
		%>
	</tbody>
</table>

<% provide(:title, "menu") %>
<% provide(:header, "menu") %>

<% if logged_in_and_current_user?(current_user) %>
  <p>
    <%= link_to "Edit My Info", edit_user_path(current_user), class: "btn btn-default form-control" %>
  </p>
  <p>
    <%= link_to "Log out", logout_path, method: :delete, class: "btn btn-default form-control" %>
  </p>
<% end %>
